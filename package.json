{
  "name": "fpms-client",
  "description": "fast package manager client for npm",
  "version": "0.0.2",
  "author": "sh4869 <nobuk4869@gmail.com>",
  "bin": {
    "fpms": "./bin/run"
  },
  "bugs": "https://github.com/ssh://git@github.com/sh4869/fpms-client.git/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@yarnpkg/lockfile": "^1.1.0",
    "debug": "^4.3.1",
    "node-fetch": "^2.6.1",
    "semver": "^7.3.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/node-fetch": "^2.5.8",
    "@types/semver": "^7.3.4",
    "@types/yarnpkg__lockfile": "^1.1.4",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "prettier": "^2.2.1",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/sh4869/fpms-client",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fpms",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ssh://git@github.com/sh4869/fpms-client.git",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
